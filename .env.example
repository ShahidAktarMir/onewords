# OneWords - Environment Configuration Template
# Copy this file to .env and update the values as needed

# Application Configuration
VITE_APP_NAME="OneWords - SSC CGL Exam Simulator"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="Ultimate MCQ - One Word Substitution Exam Simulator"

# API Configuration (if using external APIs)
# VITE_API_BASE_URL="https://api.example.com"
# VITE_API_KEY="your-api-key-here"

# Authentication Configuration (if using external auth)
# VITE_AUTH_DOMAIN="your-auth-domain"
# VITE_AUTH_CLIENT_ID="your-client-id"

# Database Configuration (if using external database)
# VITE_DATABASE_URL="your-database-url"

# Analytics Configuration (optional)
# VITE_GOOGLE_ANALYTICS_ID="GA-XXXXXXXXX"
# VITE_HOTJAR_ID="your-hotjar-id"

# Development Configuration
VITE_DEV_PORT=3000
VITE_PREVIEW_PORT=4173

# Feature Flags (optional)
# VITE_ENABLE_MOCK_DATA=true
# VITE_ENABLE_DEBUG_MODE=false
# VITE_ENABLE_ANALYTICS=false

# Build Configuration
# VITE_BUILD_SOURCEMAP=true
# VITE_BUILD_ANALYZE=false